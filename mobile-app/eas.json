{
  "cli": {
    "version": ">= 14.4.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "RUNTIME_VERSION": "2.0.0"
      },
      "channel": "development"
    },
    "preview": {
      "distribution": "internal",
      "env": {
        "RUNTIME_VERSION": "2.0.0"
      },
      "channel": "preview"
    },
    "production": {
      "autoIncrement": true,
      "env": {
        "RUNTIME_VERSION": "2.0.0"
      },
      "channel": "production"
    }
  },
  "submit": {
    "production": {}
  }
}
